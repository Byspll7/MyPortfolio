@model Experience

@{
    ViewData["Title"] = "UpdateExperience";
    Layout = "~/Views/Layout/Index.cshtml";
}





<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Experiance</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Update Experience</div>
					</div>
					<div class="card-body">


						<form method="post">
							<input type="hidden" asp-for="ExperianceID" />
							<label>About Company </label>
							<input type="text" asp-for="Head" class="form-control" />
							<br />
							<label>Title</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Experience Range</label>
							<input type="text" asp-for="Date" class="form-control" />
							<br />
							<label>Description</label>
							<textarea rows="4" class="form-control" asp-for="Description"></textarea>
							<br />
							<button class="btn btn-primary">Approve</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>

