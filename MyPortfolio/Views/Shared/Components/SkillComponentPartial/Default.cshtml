@model List<MyPortfolio.DAL.Entites.Skill>

<div class="col-six tab-full right">
    <h3>Programming Languages & Technologies</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            <li>
                <div class="progress percent @item.Value"><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }
    </ul>
</div>

